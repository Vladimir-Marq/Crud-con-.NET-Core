@model ProyectoDesarrollo.Models.Locations
@{
    ViewData["Title"] = "Details";
}

<h1>Locations</h1>

<div>
    <h4>Regions</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.LOCATION_ID)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ADDRESS)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.POSTAL_CODE)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CITY)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.STATE)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.COUNTRY_ID)
        </dd>
       
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.LOCATION_ID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
